{
  "version": 2,
  "public": true,
  "github": {
    "silent": true
  },
  "rewrites": [
    {
      "source": "/dashboard/(.*)",
      "destination": "/public/dashboard/$1"
    },
    {
      "source": "/saque/(.*)",
      "destination": "/public/saque/$1"
    },
    {
      "source": "/desbloqueio/(.*)",
      "destination": "/public/desbloqueio/$1"
    },
    {
      "source": "/faq/(.*)",
      "destination": "/public/faq/$1"
    },
    {
      "source": "/css/(.*)",
      "destination": "/public/css/$1"
    },
    {
      "source": "/js/(.*)",
      "destination": "/public/js/$1"
    },
    {
      "source": "/images/(.*)",
      "destination": "/public/images/$1"
    },
    {
      "source": "/fonts/(.*)",
      "destination": "/public/fonts/$1"
    },
    {
      "source": "/sounds/(.*)",
      "destination": "/public/sounds/$1"
    },
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/public/dashboard/index.html"
    },
    {
      "source": "/saque",
      "destination": "/public/saque/index.html"
    },
    {
      "source": "/desbloqueio",
      "destination": "/public/desbloqueio/index.html"
    },
    {
      "source": "/faq",
      "destination": "/public/faq/index.html"
    }
  ]
}